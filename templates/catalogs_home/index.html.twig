{% extends 'base.html.twig' %}

{% block title %}Hello CatalogsHomeController!{% endblock %}

{% block body %}
<div id="wrapper">
    <ol>
        {% for producer in producers %}
            <li class="system">{{ producer.name }}

                <ul>
                    {% for catalog in catalogs[producer.name] %}
                    <li class="system"><a href="{{ catalog.path }}">{{ catalog.system }}</a></li>
                    {% endfor %}
                </ul>

            </li>
        {% endfor %}
        <li class="set">
            <a href="/admin/panel">
                <img src="img/seticon.svg" alt="Settings" width="12%" height="12%">
            </a>
        </li>

    </ol>
</div>
{% endblock %}
